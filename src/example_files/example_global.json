{
    "genitive":{
        "matches":{
            "lexis":{
                "type": {
                    "match":{
                        "equals": "stem"
                    }
                },
                "tags":{
                    "match":{
                        "oneof":["genitive"]
                    }
                },
                "pos":{
                    "not":{
                        "equals":"noun"
                    }
                }
            },
            "etymon":{
                "all":{
                    "type": {
                        "match":{
                            "equals": "root"
                        }
                    }
                }
            }
        },
        "transform":[
            {
                "letter_array": {
                    "letters":[0, "Å«", 1, "u", 2]
                }
            }
        ]
    }
}